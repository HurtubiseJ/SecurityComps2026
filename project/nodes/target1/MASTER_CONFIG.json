{
    "id": "<uuid>",
    "name": "target1", 
    "enabled": true,
    "forward_host": "10.0.0.1",
    "forward_port": "8000",
    "monitor": {
        "enabled": true,
        "metrics": {
            "cpu": true,
            "memory": true,
            "disk": true,
            "network": true, 
            "fastapi": true
        },
        "interval": 1
    },
    "custom_config": {
        "caching": {
        "enabled": true,
        "static_cache": true,
        "dynamic_cache": false,
        "cache_ttl_seconds": 300,
        "cache_vary_headers": ["Accept-Encoding"],
        "cache_bypass_on_headers": ["Authorization"]
      }, 
      "rate_limit": {
        "enabled": true,
        "requests_per_ip_per_minute": 300,
        "burst": 50,
        "enforce_on_paths": ["/api"],
        "exclude_paths": ["/health"]
      }
    }


}

