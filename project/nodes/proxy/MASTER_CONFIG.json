{
  "id": "proxy-grape01",
  "name": "proxy",
  "type": "proxy",
  "enabled": true,
  "forward_host": "0.0.0.0",
  "forward_port": "8001",
  "port": "8000",
  "host": "0.0.0.0",
  "monitor": {
    "enabled": true,
    "metrics": {
      "cpu": true,
      "memory": true,
      "disk": true,
      "network": true,
      "fastapi": true
    },
    "interval": 1
  },
  "custom_config": {
    "enabled": true,
    "rate_limit_rate": "10r/s",
    "max_connections": 10,
    "burst": 10,
    "connect_timeout": "10s",
    "read_timeout": "30s",
    "send_timeout": "30s",
    "rate_limit": {
      "enabled": false
    },
    "connection_limit": {
      "enabled": false
    },
    "header_timeout": {
      "enabled": false
    },
    "tcp_hardening": {
      "enabled": false
    }
  }
}
