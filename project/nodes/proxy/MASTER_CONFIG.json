{
  "id": "proxy-grape01",
  "name": "proxy",
  "type": "proxy",
  "enabled": true,
  "forward_host": "0.0.0.0",
  "forward_port": "8001",
  "port": "8000",
  "host": "0.0.0.0",
  "monitor": {
    "enabled": true,
    "metrics": {
      "cpu": true,
      "memory": true,
      "disk": true,
      "network": true,
      "fastapi": true
    },
    "interval": 1
  },
  "custom_config": {
    "enabled": true,
    "rate_limit_rate": 10,
    "max_connections": 10,
    "burst": 10,
    "connection_timeout": 10,
    "read_timeout": 30,
    "send_timeout": 30
  }
}